  rwa [ne_nil_iff_not_if_nil, length_eq_zero_iff_is_nil]

def ft_map (c : Chains U t) [NR c] : (ft c).dom ‚ü∂ U.obj := tailMap c.chain ne_nil_of_NR

def pb [NR c] (f : d ‚ü∂ ft c) : Chains U t where
  dom := U.pb (f ‚â´ ft_map c)
  chain := Chain.cons d.dom (f ‚â´ ft_map c) d.chain

lemma gr_pb [NR c] {f : d ‚ü∂ ft c} : gr (pb f) ‚â† 0 := by
  simp [gr, pb, length]

