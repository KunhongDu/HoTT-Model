  apply congrArg _ rfl

def pb_map_aux [NR c] (f : d ‚ü∂ ft c) : (pb f).dom ‚ü∂ U.pb (ft_map c) := by
  apply PullbackCone.IsLimit.lift (@is_pullback _ _ U _ (ft_map c)) (proj (pb f) ‚â´ f) (U.pb_hmap (f ‚â´ ft_map c)) (pb_map_aux_comm f)

