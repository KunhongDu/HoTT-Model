namespace Universe
variable {C : Type u} [CategoryTheory.Category.{v, u} C] {U : Universe C}

inductive Chain (U : Universe C) : C ‚Üí C ‚Üí Type (max u v)
| nil X : Chain U X X
| cons (Y : C) (g : Y ‚ü∂ U.obj) (c : Chain U X Y)  : Chain U X (U.pb g)

open Chain
variable {X Y : C}

def toObj (_ : Chain U X Y) : C := Y

def tailObj (c : Chain U X Y) : C := by
  cases c with
  | nil => exact X
  | cons Y g c => exact Y

def tail (c : Chain U X Y) : Chain U X (tailObj c) := by
  cases c with
  | nil => exact nil X
  | cons Y g c => exact c

def toMap (c : Chain U X Y) : Y ‚ü∂ tailObj c := by
  cases c with
  | nil => exact ùüô X
  | cons Y g c => exact U.pb_vmap g

def ne_nil (c : Chain U X Y) : Prop := by
  cases c with
  | nil => exact False
  | cons => exact True

def is_nil (c : Chain U X Y) : Prop := by
  cases c with
  | nil => exact True
  | cons => exact False

lemma ne_nil_iff_not_if_nil {c : Chain U X Y} : ne_nil c ‚Üî ¬¨ is_nil c := by
  cases c with
  | nil => simp [ne_nil, is_nil]
  | cons Y g c => simp [ne_nil, is_nil]

lemma is_nil_iff_not_ne_nil {c : Chain U X Y} : is_nil c ‚Üî ¬¨ ne_nil c := by
  cases c with
  | nil => simp [ne_nil, is_nil]
  | cons Y g c => simp [ne_nil, is_nil]

def tailMap (c : Chain U X Y) (h : ne_nil c) : tailObj c ‚ü∂ U.obj := by
  cases c with
  | nil => exact False.elim h
  | cons Y g c => exact g

def length : {Y : C} ‚Üí Chain U X Y ‚Üí ‚Ñï
| _, Chain.nil _ => 0
| _, Chain.cons _ _ c => length c + 1

section LengthLemma

lemma length_eq_zero_iff_is_nil {c : Chain U X Y} : is_nil c ‚Üî length c = 0  := by
  cases c with
  | nil => simp [length, is_nil]
  | cons Y g c => simp [is_nil, length]

lemma is_nil_of_length_eq_zero {c : Chain U X Y} (h : length c = 0) : is_nil c := by rwa [length_eq_zero_iff_is_nil]

lemma length_eq_zero_of_is_nil {c : Chain U X Y} (h : is_nil c) : length c = 0  := by rwa [‚Üê length_eq_zero_iff_is_nil]

lemma eq_of_length_eq_zero {c : Chain U X Y} (h : length c = 0) : Y = X := by
  cases c with
  | nil => rfl
  | cons Y g c => simp [length] at h

lemma heq_of_length_eq_zero {c : Chain U X Y} (h : length c = 0) : HEq c (Chain.nil (U:= U) X) := by
  cases c with
  | nil => rfl
  | cons Y g c => simp [length] at h

lemma length_tail {c : Chain U X Y} : length (tail c) = length c - 1 := by
  cases c <;> rfl

lemma length_tail' {c : Chain U X Y} : length c = n + 1 ‚Üí length (tail c) = n := by intro h; rw [length_tail, h]; rfl

end LengthLemma

section Lemma

lemma tailMap_cons {g : Y ‚ü∂ U.obj} (c : Chain U X Y) : tailMap (Chain.cons Y g c) (by simp [ne_nil]) = g := rfl

end Lemma


@[ext]
structure Chains (U : Universe C) (X : C) : Type (max u v) where
  dom : C
  chain : Chain U X dom

namespace Chains

section CategoryChains

def Hom (c d : Chains U X) := c.dom ‚ü∂ d.dom

@[simp]
def id (c : Chains U X) := ùüô (c.dom)

@[simp]
def comp {c d e : Chains U X} : Hom c d ‚Üí Hom d e ‚Üí Hom c e := @CategoryStruct.comp _ _ (c.dom) (d.dom) (e.dom)

instance CategoryChains : Category (Chains U X) where
  Hom := Hom
  id := id
  comp := comp

variable {c d : Chains U X}

def toObjHom (f : c ‚ü∂ d) : c.dom ‚ü∂ d.dom := f

lemma hom_def : (c ‚ü∂ d) = (c.dom ‚ü∂ d.dom) := rfl

@[simp]
lemma id_def : ùüô c = ùüô (c.dom) := rfl

lemma eqToHom_toObjHom (h : c = d) : eqToHom h = eqToHom (congrArg dom h) := rfl

@[simp]
lemma comp_def {f : c ‚ü∂ d} {g : d ‚ü∂ e} : f ‚â´ g = CategoryStruct.comp (obj := C) f g:= rfl

end CategoryChains

section PreContextualStructure

@[simp]
def one : Chains U X where
  dom := X
  chain := Chain.nil X

@[simp]
def gr (c : Chains U X) : ‚Ñï := length c.chain

lemma one_uniq {c : Chains U X} : gr c = 0 ‚Üí c = one := by
  simp
  intro h
  ext
  simp [eq_of_length_eq_zero h]
  simp [heq_of_length_eq_zero h]

-- this is badly written
instance UniqueToTerminal (h : IsTerminal t) (X : Chains U t) : Unique (X ‚ü∂ one) := isTerminalEquivUnique _ t h _

def one_terminal (h : IsTerminal t) : IsTerminal (@one _ _ U t) := by
  have (X : Chains U t) : X ‚ü∂ one := IsTerminal.from h X.dom
  apply IsTerminal.ofUniqueHom this
  intro X m
  have : Subsingleton (X ‚ü∂ one) := @Unique.instSubsingleton _ ((UniqueToTerminal) h X)
  apply Subsingleton.allEq

@[simp]
def ft (c : Chains U X) : Chains U X where
  dom := tailObj c.chain
  chain := tail c.chain

lemma ft_one : ft (@one _ _ U X) = one := by simp [tailObj, tail]

lemma ft_gr {n : ‚Ñï} (c : Chains U X): gr c = n + 1 ‚Üí gr (ft c) = n := length_tail'

def proj (c : Chains U X) : c ‚ü∂ ft c := toMap c.chain

class isTerminal (t : C) where
  is_terminal : IsTerminal t

lemma is_terminal (t : C) [h : isTerminal t] : IsTerminal t := h.is_terminal

@[simp]
instance instChainsPreContextualCategory [h : isTerminal t] : PreContextualCategory (Chains U t) where
  Cat := by infer_instance
  gr := gr
  one := one
  one_gr := by aesop
  one_uniq := one_uniq
  one_terminal := one_terminal h.is_terminal
  ft := ft
  ft_one := ft_one
  ft_gr := ft_gr
  proj := proj

end PreContextualStructure

