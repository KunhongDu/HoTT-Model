lemma pb_map_aux_fst' [NR c] (f : d ‚ü∂ ft c) : pb_map_aux f ‚â´ (U.pb_vmap (ft_map c)) = toObjHom (proj (pb f) ‚â´ f) := by apply PullbackCone.IsLimit.lift_fst

lemma pb_map_aux_fst'' [NR c] (f : d ‚ü∂ ft c) : pb_map_aux f ‚â´ (U.pb_vmap (ft_map c)) = CategoryStruct.comp (obj := C) (proj (pb f)) f := pb_map_aux_fst _

lemma pb_map_aux_snd [NR c] (f : d ‚ü∂ ft c) : pb_map_aux f ‚â´ (U.pb_hmap (ft_map c)) = (U.pb_hmap (f ‚â´ ft_map c)) := by apply PullbackCone.IsLimit.lift_snd

